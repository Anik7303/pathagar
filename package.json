{
    "name": "pathagar",
    "version": "0.0.2",
    "description": "Server and Client app for Sudhigan Pathagar main webpage",
    "main": "server.js",
    "scripts": {
        "build:clean": "rimraf build",
        "build:client": "npm run build --prefix client",
        "build:copy": "mkdir build && cp -r ./client/build/* ./build",
        "build": "run-s build:clean build:client build:copy",
        "start": "node server.js",
        "dev": "nodemon server.js"
    },
    "devDependencies": {
        "dotenv": "^16.0.3",
        "nodemon": "^2.0.22",
        "npm-run-all": "^4.1.5",
        "rimraf": "^5.0.0"
    },
    "dependencies": {
        "express": "^4.18.2",
        "helmet": "^6.1.5",
        "multer": "^1.4.5-lts.1",
        "sequelize": "^6.31.1",
        "uuid": "^9.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/Anik7303/pathagar.git"
    },
    "keywords": [
        "Nodejs",
        "Reactjs",
        "Library"
    ],
    "author": "Anik Mohammad <anik7303@gmail.com>",
    "bugs": {
        "url": "https://github.com/Anik7303/pathagar/issues"
    },
    "homepage": "https://github.com/Anik7303/pathagar#readme",
    "license": "MIT"
}
